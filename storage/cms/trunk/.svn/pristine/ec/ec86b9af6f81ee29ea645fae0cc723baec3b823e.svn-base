.tree {
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	font-size: 11px;
	padding: 10px;
	white-space: nowrap;
}
.tree img {
	border: 0px;

	vertical-align: text-bottom;
}
.tree a {
	color: #000;
	text-decoration: none;
}
.tree a:hover {
	color: #003063;
}

td {
font-family:Arial, Helvetica, sans-serif;
color:#000000;
font-size:11px;
}

td.mil {
font-family:Arial, Helvetica, sans-serif;
color:#000000;
font-size:16px;
}

td.listResult {
font-family:Arial, Helvetica, sans-serif;
color:#000000;
font-size:10px;
align: left;
}

td.listResultInactive {
font-family:Arial, Helvetica, sans-serif;
color: red;
font-size:10px;
background-color: #DDDDDD;
align: left;
}

.console
{
font-family:Arial, Helvetica, sans-serif;
font-size:10px;
color:black;
text-transform:uppercase;
}

.success
{
font-family:Arial, Helvetica, sans-serif;
font-size:10px;
color:green;
text-transform:uppercase;
}

.error
{
font-family:Arial, Helvetica, sans-serif;
font-size:10px;
color:red;
text-transform:uppercase;
}

.component
{ 
    width:770px;
    color:#003063;
    #text-transform:uppercase;
    border-right:1px solid #003366;
    border-left:1px solid #003366;
    border-bottom:1px solid #003366;
    border-top:1px solid #003366;
}

.componentHead
{
    
    font-weight:bold;
    color:#ffffff;
    background-color: #003366;
    text-transform:uppercase;
    TEXT-ALIGN: center;
    BORDER-RIGHT: #000000 1px solid; 
    BORDER-BOTTOM: #000000 1px solid; 
}

.header_main {
	border-top:1px solid #003366;;
	border-bottom:1px solid #003366;;
	border-right:1px solid #003366;;
	border-left:1px solid #003366;;
	background-color:#eeeeee;
}

.footer {
	border-top:1px solid #003366;;
	border-bottom:1px solid #003366;;
	border-right:1px solid #003366;;
	border-left:1px solid #003366;;
	background-color:#eeeeee;
}

.dbList {
	border: 1px solid #003366;
}

.listresult 
{
    //TEXT-ALIGN: center;
    BORDER-RIGHT: #000D51 1px solid; 
    BORDER-BOTTOM: #000D51 1px solid; 
    PADDING: 0px;
    SPACING: 0px;
    text-align:left;
}

.listresultInactive 
{
    //TEXT-ALIGN: center;
    BORDER-RIGHT: #000D51 1px solid; 
    BORDER-BOTTOM: #000D51 1px solid; 
    PADDING: 0px;
    SPACING: 0px;
    text-align:left;
}

.ignoredLine 
{
    //TEXT-ALIGN: center;
    BORDER-RIGHT: #000D51 1px solid; 
    BORDER-BOTTOM: #000D51 1px solid; 
    PADDING: 0px;
    SPACING: 0px;
    text-align:left;
    background-color:#FFE4E1;
    font-size:10px;
}

.listresultMouseOver 
{
    background-color: #E8EDFA;
    //TEXT-ALIGN: center;
    BORDER-RIGHT: #000D51 1px solid; 
    BORDER-BOTTOM: #000D51 1px solid; 
}

.listHead
{
    //TEXT-ALIGN: center;
    BORDER-RIGHT: #000000 1px solid; 
    BORDER-BOTTOM: #000000 1px solid; 
    color:#003366;
    background-color:#eeeeee;
}

.textAction
{
    BORDER-RIGHT: #000000 1px solid; 
    BORDER-BOTTOM: #000000 1px solid; 
    BORDER-LEFT: #000000 1px solid; 
    BORDER-TOP: #000000 1px solid; 
    color:#FFFFFF;
    background-color:#003366;
}

a.white
{
    TEXT-ALIGN: center;
    color:#FFFFFF;
}

.dbListColumn {
border: 1px solid #000000;

}

.fctitle_blue{
    font-weight:bold;
    color:#ffffff;
    background-color: #003366;
    #text-transform:uppercase;
    width:100%;
    
}

.login {
	background-image:url(img/cms_login.gif);
	background-position:center;
	text-align:center;
	background-repeat:no-repeat;
}

body {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	background-color:#ffffff;
	color:#000000;
	margin-top:0;
	margin-bottom:0;
}

.right_main {
	vertical-align:top;
	border-left:1px solid #cccccc;
	border-right:1px solid #cccccc;
	padding-left:5px;
	width:100%;
}

a {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	color: #003366;
	text-decoration:none;

}

a.mainlink{
	color: #003366;
}
	a.mainlink:hover {
	color: #003366;
}


a:hover {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	text-decoration:underline;
}

a.sublink:hover {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#003366;
	text-decoration:none;
}

a.news:hover {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	text-decoration:underline;
}

input {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 10px;
	color: #003366;
	text-decoration: none;
}

select {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 10px;
	color: #003366;
	text-decoration: none;
}
#wrap {
    border:1px solid black;
    width:100%;
    }
#menu {
    background:#eeeeee;
    border-bottom:1px solid white;
    padding:4px 0;
    width:100%;
    }
#menu a {
    padding:4px 10px;
    text-decoration:none;
    font-weight:bold;
    color:#003366;
    }
#menu a.hover {
    background:#F4F9FF;
    }
#menu span {
    display:none;
    }
#img {
    vertical-align:bottom;
    }

#subnav {
    font-size:10px;
    margin-bottom:2em;
    }
#subnav a {
    color:#FB3B00;
    margin-right:1em;
    }
#subnav span {
    color:silver;
    margin-right:1em;
    }

