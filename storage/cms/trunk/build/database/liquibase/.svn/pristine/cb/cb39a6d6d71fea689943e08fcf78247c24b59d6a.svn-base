<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="liquibase" id="initial-setup">
        <createTable tableName="additional_benefits">
            <column autoIncrement="true" name="additional_benefit_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="benefit" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="additional_fees">
            <column autoIncrement="true" name="additional_fee_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="fee" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="additional_notes">
            <column autoIncrement="true" name="additional_note_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="note" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="alternate_links">
            <column name="affiliate_id" type="VARCHAR(8)">
                <constraints nullable="false"/>
            </column>
            <column name="clickable_id" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="url" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="website_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="approved_promotional_content">
            <column autoIncrement="true" name="approved_promotional_content_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="content" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="balance_transfers">
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="accept_partial_balance_transfer" type="INT(10)">
                <constraints nullable="false"/>
