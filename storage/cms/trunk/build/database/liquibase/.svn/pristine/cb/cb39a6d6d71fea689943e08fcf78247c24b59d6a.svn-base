                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="max_intro_period" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="intro_period_end_date" type="date">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.000" name="min_ongoing_apr" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column name="min_ongoing_apr_used_rate_type" type="VARCHAR(50)"/>
            <column defaultValueNumeric="0.000" name="max_ongoing_apr" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column name="max_ongoing_apr_used_rate_type" type="VARCHAR(50)"/>
            <column defaultValueNumeric="0.000" name="min_default_apr" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column name="min_default_apr_used_rate_type" type="VARCHAR(50)"/>
            <column defaultValueNumeric="0.000" name="max_default_apr" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column name="max_default_apr_used_rate_type" type="VARCHAR(50)"/>
            <column defaultValueNumeric="0" name="min_grace_period" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="min_finance_charge" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="min_fixed_rate_period" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="min_fee" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="max_fee" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.000" name="fee_rate" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="min_transfer_amount" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="max_transfer_amount" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.00" name="min_payment_amount" type="DECIMAL(12, 2)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0.000" name="min_payment_percentage" type="DECIMAL(6, 3)">
                <constraints nullable="false"/>
            </column>
            <column name="display_text" type="LONGTEXT"/>
        </createTable>


        <createTable tableName="card_boost">
            <column name="card_id" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="boost" type="DOUBLE"/>
        </createTable>


        <createTable tableName="card_categories">
            <column autoIncrement="true" name="card_category_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_category_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_display_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deleted" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="card_category_contexts">
            <column autoIncrement="true" name="card_category_context_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_category_context_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="card_category_group_to_category">
            <column name="card_category_group_id" type="INT(10) UNSIGNED">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_id" type="INT(10) UNSIGNED">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_group_rank" remarks="Stores ordering value of category within a group"
