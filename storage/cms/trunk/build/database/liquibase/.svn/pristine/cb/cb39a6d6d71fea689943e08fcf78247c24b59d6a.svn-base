                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="context_id" type="SMALLINT(5)"/>
            <column defaultValueComputed="'0000-00-00 00:00:00'" name="inserted" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="updated" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="card_category_ranks">
            <column autoIncrement="true" name="card_category_rank_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_category_rank" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_context_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="card_ranks">
            <column autoIncrement="true" name="card_rank_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="card_rank" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_context_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_category_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="card_tags">
            <column name="card_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="card_tag" type="VARCHAR(255)">
                <constraints nullable="false"/>
