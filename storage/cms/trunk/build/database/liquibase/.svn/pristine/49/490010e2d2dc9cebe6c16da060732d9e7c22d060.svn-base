<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <changeSet author="liquibase" id="initial-setup">
        <createTable tableName="cp_active_users">
            <column defaultValueNumeric="0" name="internalKey" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="username" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="lasthit" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="id" type="INT(10)"/>
            <column name="action" type="VARCHAR(10)">
                <constraints nullable="false"/>
            </column>
            <column name="ip" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="cp_card_category_map">
            <column defaultValueNumeric="0" name="category_id" type="INT">
                <constraints primaryKey="true"/>
            </column>
            <column name="category_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <createTable tableName="cp_category_stories">
            <column name="category_id" type="INT">
                <constraints primaryKey="true"/>
            </column>
            <column name="story_id" type="INT"/>
            <column defaultValueNumeric="0" name="disabled" type="TINYINT">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_answers">
            <column autoIncrement="true" name="answer_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="question_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="score_value" type="INT(10)"/>
            <column name="answer_text" type="LONGTEXT"/>
            <column name="explain_text" type="LONGTEXT"/>
            <column name="sort_order" type="INT(10)"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_authors">
            <column autoIncrement="true" name="author_id" type="INT(10) UNSIGNED">
                <constraints primaryKey="true"/>
            </column>
            <column name="first_name" type="VARCHAR(64)">
                <constraints nullable="false"/>
            </column>
            <column name="last_name" type="VARCHAR(64)">
                <constraints nullable="false"/>
            </column>
            <column name="address" type="LONGTEXT"/>
            <column name="business_name" type="VARCHAR(128)">
                <constraints nullable="false"/>
            </column>
            <column name="author_page_link" type="VARCHAR(255)"/>
            <column name="external_profile_link" type="VARCHAR(255)"/>
            <column name="author_photo_location" type="VARCHAR(255)"/>
            <column name="author_page_site_content_id" type="INT(10)"/>
            <column name="local_author_page_filename" type="VARCHAR(255)"/>
        </createTable>


        <createTable tableName="cp_categories">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="category" type="VARCHAR(45)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_document_groups">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="document_group" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="document" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_documentgroup_names">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="private_memgroup"
                    remarks="determine whether the document group is private to manager users" type="TINYINT(3)"/>
            <column defaultValueNumeric="0" name="private_webgroup"
                    remarks="determines whether the document is private to web users" type="TINYINT(3)"/>
            <column defaultValueNumeric="-1" name="fid" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="index_page_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_editorial_landing_page">
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="content_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="content" type="LONGBLOB"/>
            <column name="extra" type="VARCHAR(255)"/>
        </createTable>


        <createTable tableName="cp_editorial_snippets">
            <column autoIncrement="true" name="id" type="INT(10) UNSIGNED">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(100)">
                <constraints nullable="false"/>
            </column>
            <column name="text" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_evaluations">
            <column autoIncrement="true" name="evaluation_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="quiz_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="score_high_bound" type="INT(10)"/>
            <column name="score_low_bound" type="INT(10)"/>
            <column name="eval_explain_text" type="LONGTEXT"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_event_log">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="eventid" type="INT(10)"/>
            <column defaultValueNumeric="0" name="createdon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="type" remarks="1- information, 2 - warning, 3- error"
                    type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="user" remarks="link to user table" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="usertype" remarks="0 - manager, 1 - web" type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column name="source" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="LONGTEXT"/>
        </createTable>


        <createTable tableName="cp_glossary">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="term" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
            <column name="definition" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="disabled" type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="createdon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="editedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_homepage_module">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="caption" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="related" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
            <column name="story" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_jot_content">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="title" type="VARCHAR(255)"/>
            <column name="tagid" type="VARCHAR(50)"/>
            <column defaultValueNumeric="0" name="published" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="uparent" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="parent" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="flags" type="VARCHAR(25)"/>
            <column name="secip" type="VARCHAR(32)"/>
            <column name="sechash" type="VARCHAR(32)"/>
            <column name="content" type="MEDIUMTEXT"/>
            <column name="customfields" type="MEDIUMTEXT"/>
            <column defaultValueNumeric="1" name="mode" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="createdby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="createdon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="editedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="editedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deleted" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deletedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deletedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="publishedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="publishedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_jot_fields">
            <column name="id" type="MEDIUMINT(7)">
                <constraints nullable="false"/>
            </column>
            <column name="label" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="content" type="LONGTEXT"/>
        </createTable>


        <createTable tableName="cp_jot_subscriptions">
            <column autoIncrement="true" name="id" type="MEDIUMINT(7)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="uparent" type="MEDIUMINT(7)">
                <constraints nullable="false"/>
            </column>
            <column name="tagid" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="userid" type="MEDIUMINT(7)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_keyword_xref">
            <column defaultValueNumeric="0" name="content_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="keyword_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_locked_stories">
            <column autoIncrement="true" name="locked_story_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="username" remarks="The user locking a story." type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="site_content_id" remarks="The story id of the story being locked; FK to cp_site_content.id."
                    type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="locked_time"
                    remarks="The timestamp of the creation of the lock." type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_manager_log">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="timestamp" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="internalKey" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="username" type="VARCHAR(255)"/>
            <column defaultValueNumeric="0" name="action" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValue="0" name="itemid" type="VARCHAR(10)"/>
            <column name="itemname" type="VARCHAR(255)"/>
            <column name="message" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_manager_users">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="username" type="VARCHAR(100)">
                <constraints nullable="false"/>
            </column>
            <column name="password" type="VARCHAR(100)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_marquee">
            <column autoIncrement="true" name="cp_marquee_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="story_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_member_groups">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="user_group" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="member" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_membergroup_access">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="membergroup" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="documentgroup" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_membergroup_names">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="aff_id" type="VARCHAR(255)"/>
        </createTable>


        <createTable tableName="cp_poll_choices">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValueNumeric="0" name="pollid" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="choice" type="VARCHAR(255)"/>
            <column defaultValueNumeric="0" name="votes" type="INT(10)"/>
        </createTable>


        <createTable tableName="cp_poller_vote">
            <column autoIncrement="true" name="ID" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="optionID" type="INT(10)"/>
            <column name="ipAddress" type="VARCHAR(255)"/>
        </createTable>


        <createTable tableName="cp_pollip">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="pollid" type="INT(10)"/>
            <column name="ipaddress" type="VARCHAR(255)"/>
        </createTable>


        <createTable tableName="cp_polls">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(50)"/>
            <column name="question" type="VARCHAR(255)"/>
            <column defaultValueNumeric="0" name="votes" type="INT(10)"/>
        </createTable>


        <createTable tableName="cp_questions">
            <column autoIncrement="true" name="question_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="quiz_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="can_select_multi" type="TINYINT(1)"/>
            <column name="question_text" type="LONGTEXT"/>
            <column name="sort_order" type="INT(10)"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_quizzes">
            <column autoIncrement="true" name="quiz_id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="title" type="VARCHAR(255)"/>
            <column name="intro_text" type="LONGTEXT"/>
            <column name="pre_result_text" type="LONGTEXT"/>
            <column name="goodbye_text" type="LONGTEXT"/>
            <column name="default_submit_text" type="LONGTEXT"/>
            <column defaultValue="Q" name="question_plume" type="VARCHAR(255)"/>
            <column defaultValueNumeric="0" name="fid" type="INT(10)"/>
            <column name="keywords" type="LONGTEXT"/>
            <column name="description" type="LONGTEXT"/>
            <column defaultValue="Credit Card Quiz" name="actual_page_title" type="VARCHAR(255)"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="active" type="TINYINT(1)"/>
        </createTable>


        <createTable tableName="cp_redirect">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="filename" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="filepath" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="url" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="editable" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>


        <createTable tableName="cp_review">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="user_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="content_id" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="review_date" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="comments" type="LONGTEXT">
                <constraints nullable="false"/>
            </column>
        </createTable>

    </changeSet>

    <changeSet id="cp-site-content" author="kenneth">
        <createTable tableName="cp_site_content">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column defaultValue="document" name="type" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column defaultValue="text/html" name="contentType" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="author" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="pagetitle" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="longtitle" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="alias" type="VARCHAR(255)"/>
            <column name="link_attributes" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="published" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="publish_to_affiliates" type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="publish_to_cccom" type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="original_source" type="TINYINT(3)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="pub_date" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="unpub_date" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="repub_date" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="parent" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="isfolder" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="introtext" remarks="Used to provide quick summary of the document" type="LONGTEXT"/>
            <column name="content" type="MEDIUMTEXT"/>
            <column defaultValueNumeric="1" name="richtext" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="template" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="menuindex" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="1" name="searchable" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="cacheable" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="createdby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="createdon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="editedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="editedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deleted" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deletedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="deletedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="publishedon" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="publishedby" type="INT(10)">
                <constraints nullable="false"/>
            </column>
            <column name="menutitle" remarks="Menu title" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="donthit" remarks="Disable page hit count" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="haskeywords" remarks="has links to keywords" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="hasmetatags" remarks="has links to meta tags" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="privateweb" remarks="Private web document" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="privatemgr" remarks="Private manager document" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="content_dispo" remarks="0-inline, 1-attachment" type="TINYINT(1)">
                <constraints nullable="false"/>
            </column>
            <column defaultValueNumeric="0" name="hidemenu" remarks="Hide document from menu" type="TINYINT(1)">
                <constraints nullable="false"/>
